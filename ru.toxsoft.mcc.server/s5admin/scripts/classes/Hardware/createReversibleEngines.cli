createClass -classId  mcc.ThrottleValve  -parentId mcc.ReversibleEngineBase -name "Дроссельная заслонка" 

addData -id limitSwitchOpen   -type Boolean -currdata -histdata   -n "Открыт" -d "Открыт"
addData -id angle22           -type Boolean -currdata -histdata   -n "22 градуса" -d "22 градуса"
addData -id limitSwitchClose  -type Boolean -currdata -histdata   -n "Закрыт" -d "Закрыт"
addData -id angle             -type Float -currdata -histdata     -n "Угол открытия" -d "Угол открытия"
addData -id alarmVFD          -type Boolean -currdata -histdata   -n "ЧРП авария" -d "ЧРП авария"

saveChanges

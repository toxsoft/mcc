addData -id startCount        -type Integer -currdata -histdata   -n "Кол-во пусков" -d "Кол-во пусков"
addData -id powerU            -type Boolean -currdata -histdata   -n "Питание в норме" -d "Питание в норме"

addEvent       -id open    -h -name "Открыта" 
addEventParam  -ev open    -id   on     -type       Boolean 
addEvent       -id close    -h -name "Закрыта" 
addEventParam  -ev close    -id   on     -type       Boolean 
addEvent       -id powerU    -h -name "Питание в норме" 
addEventParam  -ev powerU    -id   on     -type       Boolean 
addEvent       -id limitSwitchOpen    -h -name "Открыт" 
addEventParam  -ev limitSwitchOpen    -id   on     -type       Boolean 
addEvent       -id limitSwitchClose    -h -name "Закрыт" 
addEventParam  -ev limitSwitchClose    -id   on     -type       Boolean 
addEvent       -id openSignal2DO    -h -name "Открывать" 
addEventParam  -ev openSignal2DO    -id   on     -type       Boolean 
addEvent       -id closeSignal2DO    -h -name "Закрывать" 
addEventParam  -ev closeSignal2DO    -id   on     -type       Boolean 

saveChanges

createClass -classId  mcc.PressureValve  -parentId mcc.ReversibleEngineBase -name "Задвижка нагнетания" 


addData -id open              -type Boolean -currdata -histdata   -n "Открыто" -d "Открыта"
addData -id close             -type Boolean -currdata -histdata   -n "Закрыто" -d "Закрыта"
addData -id addContactOpen    -type Boolean -currdata -histdata   -n "Открывается" -d "Открывается"
addData -id addContactClose   -type Boolean -currdata -histdata   -n "Закрывается" -d "Закрывается"

saveChanges

createClass -classId  mcc.SurgeValve  -parentId mcc.ReversibleEngineBase -name "Помпажный клапан" 

